@model IEnumerable<JobPost>
@{
    ViewData["Title"] = "Index";
}

<h2>Employer/Manage Posts</h2>


<table id="tblData" class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Job Title
            </th>
            <th>
                Department
            </th>
            <th>
                Weekly hours
            </th>
            <th>
                Start Date
            </th>
            <th>
                End Date
            </th>
            <th>
                Hourly Rate
            </th>
            <th>
                Closing Date
            </th>
            <th>
                Apply
            </th>
        </tr>
    </thead>
</table>

<div>
    <a asp-area="Student" asp-action="Index" class="btn btn-secondary" style="width:300px">Back</a>
</div>

@section Scripts{
    <script src="~/js/StudentsPost.js"></script>
}


